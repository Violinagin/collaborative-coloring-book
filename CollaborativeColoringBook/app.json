{
  "expo": {
    "name": "ForkArt",
    "slug": "ForkArt",
    "scheme": "forkart",
    "version": "1.5.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/65c4cb38-e6c9-41c8-8dee-c962ffc452c7",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "art.forkart.app",
      "buildNumber": "9",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos for upload.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select images for upload.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 9,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "art.forkart.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos."
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24
          },
         "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]      
    ],
    "extra": {
      "eas": {
        "projectId": "65c4cb38-e6c9-41c8-8dee-c962ffc452c7"
      }
    },
    "owner": "violinagin"
  }
}
