{
  "name": "forkart",
  "version": "1.0.0",
  "main": "app-polyfills.js",
  "scripts": {
      "start": "NODE_OPTIONS=\"--require ./preload-polyfills.js\" EXPO_NO_TELEMETRY=1 npx expo start --clear --max-workers=1",
      "start:lan": "NODE_OPTIONS=\"--require ./preload-polyfills.js\" EXPO_NO_TELEMETRY=1 REACT_NATIVE_PACKAGER_HOSTNAME=192.168.4.22 npx expo start --lan --clear --max-workers=1",
      "start:ipad": "NODE_OPTIONS=\"--require ./preload-polyfills.js\" EXPO_NO_TELEMETRY=1 npx expo start --tunnel --clear",
      "clean": "rm -rf $TMPDIR/metro-* $TMPDIR/haste-* .expo node_modules/.cache",
      "dev": "npm run clean && npm start",
    "android": "expo start --android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/native": "^11.11.0",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-vector-icons/material-icons": "^12.4.0",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/native": "^7.1.21",
    "@react-navigation/native-stack": "^7.7.0",
    "@react-navigation/stack": "^7.6.11",
    "@supabase/supabase-js": "^2.84.0",
    "@types/react": "~19.1.10",
    "base64-arraybuffer": "^1.0.2",
    "expo": "^54.0.30",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-image-picker": "^17.0.10",
    "expo-secure-store": "^15.0.8",
    "expo-status-bar": "^3.0.9",
    "expo-system-ui": "^6.0.9",
    "expo-updates": "^29.0.15",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "typescript": "~5.9.2"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "private": true
}
